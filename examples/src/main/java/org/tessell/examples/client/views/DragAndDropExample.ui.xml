<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="org.tessell.examples.client.views.DragAndDropExampleStyle">
	.bold {
	  font-weight: bold;
	}
	</ui:style>
	<gwt:FlowPanel>
		<gwt:FlowPanel ui:field="rootLabels">
			<gwt:Label ui:field="l1" text="One" />
			<gwt:Label ui:field="l2" text="Two" />
			<gwt:Label ui:field="l3" text="Three" />
			<gwt:Label ui:field="l4" text="Four" />
		</gwt:FlowPanel>
		<gwt:FlowPanel ui:field="rootAnchors">
			<gwt:Anchor ui:field="a1" text="One" />
			<gwt:Anchor ui:field="a2" text="Two" />
			<gwt:Anchor ui:field="a3" text="Three" />
			<gwt:Anchor ui:field="a4" text="Four" />
		</gwt:FlowPanel>
	</gwt:FlowPanel>
</ui:UiBinder>
